{
  "name": "pokemon-app",
  "homepage": "https://ss-520.github.io/pokemon-app/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "vite-dev": "vite",
    "dev": "npm-run-all -p vite-dev sync",
    "sync": "browser-sync start --proxy http://localhost:5173 --files 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --no-open",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d build -r -m \"Updates --skip-ci\""
  },
  "dependencies": {
    "eslint-plugin-neverthrow": "^1.1.4",
    "neverthrow": "^8.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "ts-results": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "browser-sync": "^3.0.4",
    "cssnano": "^7.1.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-sort-media-queries": "^5.2.0",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  }
}
